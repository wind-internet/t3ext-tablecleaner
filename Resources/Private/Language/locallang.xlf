<xliff version="1.0">
    <file source-language="en" datatype="plaintext" original="messages" date="2013-11-26T17:55:17Z"
          product-name="tablecleaner">
        <header/>
        <body>
            <trans-unit id="mlang_labels_tabdescr">
                <source>Overview of pages excluded from the cleaning process.</source>
            </trans-unit>
            <trans-unit id="mlang_tabs_tab">
                <source>Table Cleaner</source>
            </trans-unit>
            <trans-unit id="module.description">
                <source>Overview of pages excluded from the cleaning process. Here you will find a tree including all
                    the pages that have 'exclude from cleaning' or 'exclude branch' from cleaning set in their page
                    properties. Clicking on a 'page title' will bring up the page properties for that page.
                </source>
            </trans-unit>
            <trans-unit id="module.startingPoint">
                <source>Showing excluded page from starting point '%s' (uid: %s)</source>
            </trans-unit>
            <trans-unit id="module.subTitle">
                <source>Overview of excluded pages</source>
            </trans-unit>
            <trans-unit id="module.title">
                <source>Table Cleaner</source>
            </trans-unit>
            <trans-unit id="module.usage">
                <source>Usage</source>
            </trans-unit>
            <trans-unit id="module.usage.description">
                <source>Select a page in the page tree as starting point.</source>
            </trans-unit>
            <trans-unit id="module.usage.exclude">
                <source>Page is excluded from cleaning</source>
            </trans-unit>
            <trans-unit id="module.usage.excludeBranch">
                <source>Page and all child pages are excluded from cleaning</source>
            </trans-unit>
            <trans-unit id="tasks.deleted.title">
                <source>Remove deleted entries</source>
            </trans-unit>
            <trans-unit id="tasks.deleted.description">
                <source>Removes deleted entries older than N days from tables.</source>
            </trans-unit>
            <trans-unit id="tasks.deleted.dayLimit">
                <source>Remove deleted entries older than given number of days</source>
            </trans-unit>
            <trans-unit id="tasks.limit">
                <source>Maximum amount of rows to handle per run</source>
            </trans-unit>
            <trans-unit id="tasks.deleted.additionalInformation">
                <source>Remove deleted entries older than %s days from tables: %s in batches of %d</source>
            </trans-unit>
            <trans-unit id="tasks.expired.title">
                <source>Remove expired entries</source>
            </trans-unit>
            <trans-unit id="tasks.expired.description">
                <source>Removes entries older than N days from tables.</source>
            </trans-unit>
            <trans-unit id="tasks.expired.dayLimit">
                <source>Delete entries older than given number of days</source>
            </trans-unit>
            <trans-unit id="tasks.expired.additionalInformation">
                <source>Remove entries older than %s days from tables: %s in batches of %d</source>
            </trans-unit>
            <trans-unit id="tasks.general.additionalInformationExclude">
                <source>except from pages %s</source>
            </trans-unit>
            <trans-unit id="tasks.general.additionalInformationExcludeRecursive">
                <source>recursively</source>
            </trans-unit>
            <trans-unit id="tasks.tables">
                <source>Tables</source>
            </trans-unit>
            <trans-unit id="tasks.hidden.markAsDeleted">
                <source>Mark records as 'deleted' instead of removing them from the table</source>
            </trans-unit>
            <trans-unit id="tasks.general.noTables">
                <source>Please select one or more tables to clean</source>
            </trans-unit>
            <trans-unit id="tasks.general.invalidLimit">
                <source>Please specify an integer for the limit field</source>
            </trans-unit>
            <trans-unit id="tasks.general.invalidTables">
                <source>You somehow selected a table that is not available</source>
            </trans-unit>
            <trans-unit id="tasks.general.invalidNumberOfDays">
                <source>Please enter a valid number of days</source>
            </trans-unit>
            <trans-unit id="tasks.optimize">
                <source>Perform 'OPTIMIZE TABLE' after clean up&lt;br/&gt;(WARNING: database may be unavailable
                    shortly!)
                </source>
            </trans-unit>
            <trans-unit id="tasks.hidden.title">
                <source>Remove hidden entries</source>
            </trans-unit>
            <trans-unit id="tasks.hidden.description">
                <source>Removes hidden entries older than N days from tables.</source>
            </trans-unit>
            <trans-unit id="tasks.hidden.dayLimit">
                <source>Remove hidden entries older than given number of days</source>
            </trans-unit>
            <trans-unit id="tasks.hidden.additionalInformation">
                <source>Remove hidden entries older than %s days from tables: %s in batches of %d</source>
            </trans-unit>
        </body>
    </file>
