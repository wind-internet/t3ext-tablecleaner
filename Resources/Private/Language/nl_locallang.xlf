                <source>Maximaal aantal te bewerken rijen</source>
            </trans-unit>
            <trans-unit id="tasks.deleted.additionalInformation">
                <target>Verwijder 'verwijderde' rijen ouder dan %s dagen van tabellen: %s in batches van %d</target>
            </trans-unit>
            <trans-unit id="tasks.expired.title">
                <target>Tabel Opschonen: verouderde rijen</target>
            </trans-unit>
            <trans-unit id="tasks.expired.description">
                <target>Verwijdert rijen ouder dan N dagen van tabellen.</target>
            </trans-unit>
            <trans-unit id="tasks.expired.dayLimit">
                <target>Verwijder rijen ouder dan het opgegeven aantal dagen</target>
