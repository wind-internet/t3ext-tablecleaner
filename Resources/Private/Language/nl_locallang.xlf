                    kun je de pagina eigenschappen van die pagina bewerken.
                </target>
            </trans-unit>
            <trans-unit id="module.startingPoint">
                <target>Pagina's vanaf startpunt '%s' (uid: %s)</target>
            </trans-unit>
            <trans-unit id="module.subTitle">
                <target>Overzicht van uitgesloten pagina's</target>
            </trans-unit>
            <trans-unit id="module.title">
                <target>Table Opschoner</target>
            </trans-unit>
            <trans-unit id="module.usage">
                <target>Gebruik</target>
            </trans-unit>
            <trans-unit id="module.usage.description">
                <target>Kies een pagina uit de paginaboom als startpunt.</target>
            </trans-unit>
            <trans-unit id="module.usage.exclude">
                <target>Pagina is uitgesloten van opschoning</target>
            </trans-unit>
